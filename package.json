{
  "name": "ngx-feature-toggle",
  "private": false,
  "description": "Your module to handle with feature toggles in Angular applications easier.",
  "author": "Will Mendes <willmendesneto@gmail.com>",
  "license": "MIT",
  "version": "4.1.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "ng2",
    "ngx",
    "feature-toggle",
    "ngx-feature-toggle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/willmendesneto/ngx-feature-toggle.git"
  },
  "bugs": {
    "url": "https://github.com/willmendesneto/ngx-feature-toggle/issues"
  },
  "homepage": "https://github.com/willmendesneto/ngx-feature-toggle#readme",
  "scripts": {
    "editorconfig-tools-check": "editorconfig-tools check ./src/** ./demo/**",
    "check-package-license-list": "licensee &> /dev/null",
    "bundle-report": "npm run build-report && npm run analyze dist/bundles/ngx-feature-toggle.umd.js",
    "lite-server": "lite-server -c demo/bs-config.json",
    "analyze": "source-map-explorer",
    "demo-report": "npm run build-package-link && npm run demo-build-report",
    "demo-serve": "npm run build-package-link && npm run demo-build && npm run lite-server",
    "demo-dev-serve": "npm run build-package-link && ng serve",
    "demo-gh-pages": "npm run build && npm run demo-build && npm run demo-deploy",
    "demo-build-report": "ng build --clean --prod --aot --sm",
    "demo-build": "ng build -prod --aot && npm run demo-copy-manifest && npm run demo-generate-version",
    "demo-copy-manifest": "cp -f demo/src/manifest.json demo/dist",
    "demo-generate-version": "echo \"$(git rev-parse HEAD)\" > demo/dist/VERSION",
    "demo-deploy": "gh-pages -d demo/dist",
    "link": "ngm link -p src --here",
    "lint": "tslint \"src/**/*.ts\" -c tslint.json",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "build-report": "ngm build -p src --clean --prod --aot --sm",
    "build": "ngm build -p src --clean",
    "prepublish": "npm run build",
    "build-watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot",
    "commit": "git-cz",
    "build-package-link": "npm run build && npm run link",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "npm run lint && npm run build-package-link",
    "test": "ng test -sr",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf coverage",
    "version": "npm run changelog && git add -A",
    "bundlesize": "bundlesize"
  },
  "peerDependencies": {
    "@angular/common": "^2.3.1 || >=4.0.0",
    "@angular/core": "^4.2.5"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/cli": "^6.0.0-beta.5",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^5.0.0",
    "@angular/language-service": "^5.0.1",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^5.0.0",
    "@angular/service-worker": "^5.0.1",
    "@angular/tsc-wrapped": "^4.4.6",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.8.3",
    "@types/marked": "0.3.0",
    "@types/node": "8.0.58",
    "@types/webpack": "3.8.2",
    "bundlesize": "^0.15.3",
    "classlist-polyfill": "1.2.0",
    "codelyzer": "~4.0.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "1.3.5",
    "conventional-github-releaser": "2.0.0",
    "core-js": "^2.4.1",
    "coveralls": "^3.0.0",
    "cpy": "5.1.0",
    "cpy-cli": "1.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "editorconfig-tools": "^0.1.1",
    "gh-pages": "1.1.0",
    "hammerjs": "^2.0.8",
    "jasmine": "2.7.0",
    "jasmine-core": "2.8.0",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "4.2.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "0.6.0",
    "karma-sauce-launcher": "1.1.0",
    "licensee": "^4.0.1",
    "lite-server": "2.3.0",
    "ngm-cli": "0.5.2",
    "protractor": "5.2.1",
    "semantic-release": "^11.0.2",
    "source-map-explorer": "^1.3.3",
    "ts-helpers": "^1.1.1",
    "ts-node": "4.0.2",
    "tslint": "5.5.0",
    "tslint-config-valorsoft": "2.1.1",
    "typedoc": "0.8.0",
    "typescript": "2.6.2",
    "uglify-js": "^3.0.28",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.19"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6.10.2"
  },
  "dependencies": {
    "feature-toggle-service": "^2.0.1"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ngx-feature-toggle.umd.min.js",
      "threshold": "1.45 KB"
    }
  ]
}
